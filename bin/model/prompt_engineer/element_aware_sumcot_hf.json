{
  "llm_engine_type": "huggingface",
  "llm_engine_api": "",
  "llm": "meta-llama/Meta-Llama-3-8B-Instruct",
  "data_path_or_name": "EdinburghNLP/xsum",
  "data_split": "test",
  "output_path": "./_element_aware_sumcot_inf.jsonl",
  "input_text_col": "document", 
  "target_text_col": "summary", 
  "element_queries": [
    "What are the important entities in this document?",
    "What are the important dates in this document?",
    "What events are happening in this document?"
  ],
  "maximum_output_words": 35,
  "sampling": true, 
  "temperature": 0.01,
  "top_k": 1, 
  "dbg_mode": true
}
